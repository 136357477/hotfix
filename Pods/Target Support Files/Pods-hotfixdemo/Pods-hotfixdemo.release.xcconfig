FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/BuglyHotfix"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/BuglyHotfix" "${PODS_ROOT}/Headers/Public/JSPatch"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/JSPatch"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/BuglyHotfix" -isystem "${PODS_ROOT}/Headers/Public/JSPatch"
OTHER_LDFLAGS = $(inherited) -ObjC -l"JSPatch" -l"c++" -l"z" -framework "BuglyHotfix" -framework "Foundation" -framework "JavascriptCore" -framework "Security" -framework "SystemConfiguration" -weak_framework "JavaScriptCore"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
